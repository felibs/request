{
  "name": "@felibs/request",
  "version": "0.1.2",
  "description": "hooks for vue3.x, easy to use request or async function, inspired by umijs/hooks",
  "main": "dist/request.cjs.js",
  "types": "dist/request.d.ts",
  "module": "dist/request.esm-bundler.js",
  "unpkg": "dist/request.global.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "serve": "serve",
    "test": "jest --coverage",
    "build": "node scripts/build.js",
    "dev": "node scripts/dev.js",
    "release": "node scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felibs/reqeust.git"
  },
  "author": "daboge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felibs/reqeust/issues"
  },
  "homepage": "https://github.com/felibs/reqeust#readme",
  "gitHooks": {
    "pre-commit": "ls-lint && lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "{lib,__tests__}/*.js": [
      "prettier --tab-width 4 --write",
      "git add"
    ],
    "{lib,__tests__}/*.ts?(x)": [
      "prettier --parser=typescript --tab-width 4 --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.8.1",
    "@microsoft/api-extractor": "^7.7.13",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/jest": "^25.2.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/sinon": "^9.0.0",
    "bl": "^4.0.2",
    "conventional-changelog-cli": "^2.0.31",
    "enquirer": "^2.3.5",
    "execa": "^4.0.0",
    "fs-extra": "^9.0.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.7",
    "minimist": "^1.2.5",
    "prettier": "^2.0.5",
    "rollup": "^2.6.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "semver": "^7.3.2",
    "serve": "^11.3.0",
    "sinon": "^9.0.2",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "vue": "3.0.0-beta.3"
  }
}
